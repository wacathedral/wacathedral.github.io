=flex-display
    display: -webkit-box
    display: -moz-box
    display: ms-flexbox
    display: flex
    

=flex-direction($v)
    -webkit-box-orient: horizontal
    -webkit-box-direction: normal
    -webkit-flex-direction: $v
    -ms-flex-direction: $v
    flex-direction: $v


=justify-content($v)
    -webkit-box-pack: $v
    -webkit-justify-content: $v
    -ms-flex-pack: $v
    justify-content: $v

=align-items($v)
    -webkit-box-align: $v
    -webkit-align-items: $v
    -ms-flex-align: $v
    align-items: $v

=flex-wrap($v)
    -webkit-flex-wrap: $v
    -ms-flex-wrap: $v
    flex-wrap: $v


//wrap, nowrap


=flex($v)
    -webkit-box-flex: $v
    -webkit-flex: $v
    -ms-flex: $v
    flex: $v
